{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe C++20 build file",
			"command": "C:\\TDM-GCC-64\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++20",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: C:\\TDM-GCC-64\\bin\\g++.exe"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: clang++.exe C++20 build file",
			"command": "C:\\Program Files\\LLVM\\bin\\clang++.exe",
			"args": [
				"-fcolor-diagnostics",
				"-fansi-escape-codes",
				"-std=c++20",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: \"C:\\Program Files\\LLVM\\bin\\clang++.exe\""
		},
		{
			"type": "cppbuild",
			"label": "C/C++: clang-cpp.exe C++20 build file",
